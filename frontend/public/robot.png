<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Robot body -->
  <rect x="50" y="80" width="100" height="90" rx="5" fill="#667eea" stroke="#333" stroke-width="2"/>
  
  <!-- Robot head -->
  <circle cx="100" cy="50" r="35" fill="#667eea" stroke="#333" stroke-width="2"/>
  
  <!-- Left eye -->
  <circle cx="85" cy="40" r="6" fill="white" stroke="#333" stroke-width="1"/>
  <circle cx="85" cy="40" r="3" fill="#333"/>
  
  <!-- Right eye -->
  <circle cx="115" cy="40" r="6" fill="white" stroke="#333" stroke-width="1"/>
  <circle cx="115" cy="40" r="3" fill="#333"/>
  
  <!-- Mouth -->
  <path d="M 90 55 Q 100 60 110 55" stroke="#333" stroke-width="2" fill="none" stroke-linecap="round"/>
  
  <!-- Left arm -->
  <rect x="20" y="95" width="30" height="20" rx="5" fill="#764ba2" stroke="#333" stroke-width="2"/>
  
  <!-- Right arm -->
  <rect x="150" y="95" width="30" height="20" rx="5" fill="#764ba2" stroke="#333" stroke-width="2"/>
  
  <!-- Left leg -->
  <rect x="60" y="170" width="20" height="25" rx="3" fill="#764ba2" stroke="#333" stroke-width="2"/>
  
  <!-- Right leg -->
  <rect x="120" y="170" width="20" height="25" rx="3" fill="#764ba2" stroke="#333" stroke-width="2"/>
  
  <!-- Left foot -->
  <rect x="55" y="195" width="30" height="8" rx="2" fill="#333"/>
  
  <!-- Right foot -->
  <rect x="115" y="195" width="30" height="8" rx="2" fill="#333"/>
  
  <!-- Antenna -->
  <line x1="100" y1="15" x2="100" y2="5" stroke="#333" stroke-width="2" stroke-linecap="round"/>
  <circle cx="100" cy="2" r="3" fill="#764ba2" stroke="#333" stroke-width="1"/>
</svg>
